{
    "identifier": "Mecham.ESLint",
    "name": "ESLint",
    "author": "Justin Mecham",
    "description": "Integrates with ESLint to provide contextual issues in Nova.",
    "version": "1.0",
    "main": "main.js",
    "activationEvents": [
        "onLanguage:javascript",
        "onLanguage:typescript",
        "onLanguage:jsx",
        "onLanguage:tsx",
        "onWorkspaceContains:.eslintrc.*",
    ],
    "entitlements": {
        "process": true
    }
}
